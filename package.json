{
  "name": "rudydahbura.auth0.com",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Auth0 deployment repository.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdahbura/rudydahbura.auth0.com.git"
  },
  "scripts": {
    "format": "prettier --write './{clients,connections,database-connections,emails,grants,guardian,hooks,pages,resource-servers,roles,rules}/**/*.{css,html,js,json,jsx}'"
  },
  "dependencies": {
    "aws-sdk": "^2.995.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "engines": {
    "node": "16",
    "yarn": "1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
